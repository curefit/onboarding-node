{
  "name": "express_server",
  "version": "1.0.0",
  "description": "Express node http server",
  "main": "index.js",
  "author": "hmittal",
  "license": "MIT",
  "scripts": {
    "clean-dist": "rm -f -r -d dist",
    "start-dev": "tsc --skipLibCheck && node dist/index.js",
    "build": "npm run clean-dist && tsc --skipLibCheck && npm run copy-configs",
    "build-start-dev": "npm run build  && npm run start-dev"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.2",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.3.1",
    "tslib": "^2.3.0"
  }
}
